{
	"name": "cfn-resource-actions",
	"displayName": "CloudFormation Resource Actions",
	"description": "Integrate with deployed resources directly from your CloudFormation template",
	"version": "0.0.5",
	"publisher": "ljacobsson",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"keywords": ["CloudFormation"],
	"repository": {
		"type": "git",
		"url": "https://github.com/mhlabs/cfn-resource-actions.git"
	},
	"activationEvents": [
		"onCommand:cfn-resource-actions.enableCodeLens"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cfn-resource-actions.enableCodeLens",
				"title": "CloudFormation Resource Actions"
			}
		],
		"configuration": {
			"title": "CloudFormation Resource Actions",
			"properties": {
				"cfn-resource-actions.stackName": {
					"type": "string",
					"default": null,
					"description": "The name of the deployed stack that you want to map your template with. (Should be set in workspace settings)"
				},
				"cfn-resource-actions.enableCodeLens": {
					"type": "boolean",
					"default": false,
					"description": "Enable or disable this feature"
				},
				"cfn-resource-actions.sso.useSSO": {
					"type": "boolean",
					"default": false,
					"description": "Enable or disable AWS SSO authentication"
				},
				"cfn-resource-actions.sso.startUrl": {
					"type": "string",
					"default": null,
					"description": "The AWS SSO start URL"
				},
				"cfn-resource-actions.sso.accountId": {
					"type": "string",
					"default": null,
					"description": "Your account ID"
				},
				"cfn-resource-actions.sso.region": {
					"type": "string",
					"default": null,
					"description": "Your AWS region"
				},
				"cfn-resource-actions.sso.role": {
					"type": "string",
					"default": null,
					"description": "The SSO role to assume"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@mhlabs/aws-sso-client-auth": "^1.0.7",
		"@types/opn": "^5.5.0",
		"aws-sdk": "^2.709.0",
		"opn": "^6.0.0",
		"yaml-cfn": "^0.2.3"
	}
}